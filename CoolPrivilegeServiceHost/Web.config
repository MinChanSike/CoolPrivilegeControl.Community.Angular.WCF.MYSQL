    <add key="LangPackProjectName" value="CoolPrivilegeControlLangPack" />
    
    <add key="DBServer" value="mysql" />
    <add key="DBSource" value="127.0.0.1" />
    <add key="DBName" value="coolprivilegecontroldb" />
